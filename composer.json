  "require": {
    "symfony/finder": "^6.2",
    "symfony/console": "^6.2",
    "symfony/dependency-injection": "^6.2",
    "symfony/config": "^6.2",
    "symfony/yaml": "^6.2"
  },
  "require-dev": {
